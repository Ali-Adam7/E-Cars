// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model PO {
    id     Int    @id @default(autoincrement())
    userID Int
    status String
}

model POItem {
    id       Int
    vid      Int
    price    Int
    quantity Int

    @@id([id, vid])
}
