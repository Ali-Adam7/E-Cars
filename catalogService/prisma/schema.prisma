// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Car {
  id      Int     @id @default(autoincrement())
  model   String
  make    String
  type    String
  price   Decimal
  img     String
  year    Int
  history String
  milage  Decimal
}
